{
  "name": "hipr",
  "version": "1.0.1",
  "productName": "hipr",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "package": "electron-packager .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && mv dist ../back && electron ."
